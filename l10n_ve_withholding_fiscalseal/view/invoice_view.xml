<?xml version='1.0' encoding='UTF-8'?>
<openerp>
    <data>
        <record id="account_invoice_wh_iva_supplier" model="ir.ui.view">
            <field name="name">account.invoice.wh.iva.supplier</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="l10n_ve_withholding.wh_inv_supplier"/>
            <field name="arch" type="xml">
                <xpath expr="//separator[@string='fsep']" position="after">
                    <newline/>
                    <group col="4" colspan="4">
                        <field
                            name="fiscalseal_apply"
                            string="Exclude this document from Fiscal Seal Withholding"/>
                        <button
                            name="%(action_wiz_fiscalseal_apply_wh)d"
                            string="Change Fiscal Seal Wh. Exclusion"
                            type="action"
                            icon="gtk-execute,open"
                            states="open"
                            />
                        <separator orientation="vertical" colspan="4"/>
                        <field name="wh_fiscalseal" string="Have this invoice been already Fiscal Seal withheld?"/>
                        <field name="wh_fiscalseal_id"/>
                    </group>
                </xpath>
                <xpath expr="//separator[@string=&quot;fsep&quot;]" position="attributes">
                    <attribute name="string">Fiscal Seal Withholding</attribute>
                    <attribute name="invisible">0</attribute>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
